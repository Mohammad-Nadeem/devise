    <!--form class="login-form" -->
    <label class="l"><h4>Login<h4></label>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

       <div class="login-page">
       <div class="form">
       <%#= f.label :email %><br />
       <%= f.email_field :email, autofocus: true , placeholder: 'Email'%>
      <!-- <input type="text" placeholder="username"/> -->
       <%#= f.label :password %><br />
       <%= f.password_field :password, autocomplete: "off" , placeholder: 'Password'%>
      <!-- <input type="password" placeholder="password"/> -->
       <div class="ab">
       <%= f.check_box :remember_me %>
       <%= f.label :remember_me %>
       </div>
       <div class="actions bt">
         <%= f.submit "Log in" %>
       </div>
            <!-- <button>login</button> -->
            <div class="link"> <%= link_to "Forgot your password?", new_password_path(resource_name) %><br /></div>

      <p class="message">Not registered? <%= link_to "Create an account", new_registration_path(resource_name) %><br/></p>
     
    <!--/form -->
    <%#= render "devise/shared/links" %>
  </div>

</div>

 <% end %>




